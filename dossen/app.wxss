/**app.wxss**/
page{
   background-color: #efefef;
}
.container {
  width: 100%;
  height: 100%;
  /*display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;*/
  padding: 0;
  margin: 0;
  box-sizing: border-box;
  background-color: #efefef;
}

.footer {
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  padding: 10rpx 0;
  background-color: #fff;
  border-top: 1px solid #eee;
  display: flex;
  align-items: center;
}

.footer>view {
  flex: 1;
}

.footer>view image {
  width: 46rpx;
  height: 46rpx;
  margin: 0 auto;
  display: block;
}

.footer>view text {
  display: block;
  text-align: center;
  font: 26rpx/2 "Microsoft YaHei", Arial, Helvetica, sans-serif;
  color: #888;
}


.nullOrder{
  margin-top:50%;
  text-align: center;
}
.nullOrder view{
  margin:0 auto;
  width:200rpx;
  height:200rpx;
}
.nullOrder view image{
  width:100%;
  height:100%;
}
.nullOrder text{
  display: block;
  font: 30rpx/2 "Microsoft YaHei", Arial, Helvetica, sans-serif;
  color:#666;
}


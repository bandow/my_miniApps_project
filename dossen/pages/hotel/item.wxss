.address .left {
  margin-right: 20rpx;
}

.address .left .imgL {
  width: 30rpx;
  height: 40rpx;
}

.address .right {
  flex: 5;
}

.address .right text {
  display: block;
  font: 30rpx/1 "Microsoft YaHei", Arial, Helvetica, sans-serif;
  color: #666;
}

.address .right .addressText {
  color: #2d2d2d;
  margin-bottom: 15rpx;
}

.grade {
  position: relative;
  padding: 3% 10% 3% 5%;
  display: -webkit-flex;
  display: flex;
  align-items: center;
  background-color: #fff;
  border-bottom: 1px solid #eee;
}

/*.grade:after{
    content:"";
    position: absolute;
    top:50%;
    right:5%;
    margin-top:-3px;
    width:6px;
    height:6px;
    border: 1px solid;

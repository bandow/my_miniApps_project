/* pages/hotel/detailed.wxss */

.carousel {
  position: relative;
}

.slide-image {
  width: 100%;
  height: 100%;
}

.carousel view image {
  width: 100%;
  height: 100%;
}

.iconPhone {
  position: absolute;
  top: 5%;
  right: 5%;
  width: 40rpx;
  height: 40rpx;
}

.iconCollect {
  position: absolute;
  top: 5%;
  right: 5%;
  width: 40rpx;
  height: 40rpx;
}

.carousel text {
  position: absolute;
  bottom: 5%;
  left: 5%;
  font: 30rpx/1.2 "Microsoft YaHei", Arial, Helvetica, sans-serif;
  color: #fff;
}

.address {
  position: relative;
  padding: 3% 5% 1%;
  display: -webkit-flex;
  display: flex;
  align-items: center;
  background-color: #fff;
  border-bottom: 1px solid #eee;
}


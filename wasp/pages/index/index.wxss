/**index.wxss**/
page{
  background: #fff;
}
.text-black-bg-fed426-color{
  color:#fff;
  background:black;
}
.text-black-color-fed426-bg{
  color:black;
  background:#fff;
}
.container{
  position:relative;
  display:inherit;
  padding: 0;
  font-size: 14rpx;
  background: #fff;
  color: #999999;
}
.container .header{
  position:relative;
  height:auto;
  text-align:center;
  background:#fff;
}
.container .header image{
  width:100%;
}
.container .header .logoimg{
  display:block;
  padding-top:16vh;
  padding-bottom:0rpx;
  margin:auto;
  width:420rpx;
  height:auto;
}
.container .search{
  position:relative;
  margin:-120rpx 40rpx 0 40rpx;
  padding-right:98rpx;
  background: #fff;
  border:5rpx solid #fed426;
  border-radius:30px;
  overflow:hidden;
}

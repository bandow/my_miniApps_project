/* reset */
page {
  background-color: #e8edf1;
  height: 100%;
  font-size: 32rpx;
  line-height: 1.6;
  font-family: -apple-system-font, 'Helvetica Neue', Helvetica, 'Microsoft YaHei', sans-serif;
}
checkbox, radio{
  margin-right: 10rpx;
}
button{
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}
form{
  width: 100%;
}

/* lib */
.strong{
  font-weight: bold;
}
.tc{
  text-align: center;
}

/* page */
.container {
  display: flex;
  flex-direction: column;
  height: 100%;
  justify-content: space-between;
  font-size: 32rpx;
}

text{
  display:block;
  font-size:15px;
}
